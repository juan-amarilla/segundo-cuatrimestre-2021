#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int numeroUno;
    int numeroDos;
	int numeroTres;

    printf("Ingrese el primer numero: \n ");
	fflush(stdout);
	scanf("%d", &numeroUno);

	printf("Ingrese el segundo numero: \n ");
	fflush(stdout);
	scanf("%d", &numeroDos);

	printf("Ingrese el tercer numero: \n ");
	fflush(stdout);
	scanf("%d", &numeroTres);

	if(numeroUno > numeroDos && numeroUno < numeroTres)
	{
		printf(" ");

	}

	return 0;
}
