#include <stdio.h>

int main()
{
    int numeroUno;
    int numeroDos;
    int resultado;

    fflush(stdout);

    printf("Ingrese el primer numero entero:\n ");
    scanf("%d", &numeroUno);

    printf("Ingrese el segundo numero entero:\n ");
    scanf("%d", &numeroDos);

    resultado = numeroUno + numeroDos;

    printf("El numero resultante es: %d", resultado);

    return 0;
}

